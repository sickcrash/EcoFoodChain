{
    "name":  "refood-frontend",
    "homepage": "/refood",
    "main":  "expo-router/entry",
    "version":  "1.0.0",
    "scripts":  {
                    "start":  "expo start",
                    "start:clear":  "expo start --clear",
                    "reset-project":  "node ./scripts/reset-project.js",
                    "android":  "expo run:android",
                    "ios":  "expo run:ios",
                    "web":  "expo start --web",
                    "test":  "jest --watchAll",
                    "lint":  "expo lint",
                    "typecheck":  "tsc -p . --noEmit",
                    "e2e:setup":  "npx playwright install",
                    "e2e:smoke":  "npx playwright test --config tests/e2e/playwright/playwright.config.ts",
                    "e2e:open":  "npx playwright test --config tests/e2e/playwright/playwright.config.ts --ui"
                },
    "jest":  {
                 "preset":  "jest-expo",
                 "setupFilesAfterEnv":  [
                                            "\u003crootDir\u003e/jest.setup.ts"
                                        ],
                 "testPathIgnorePatterns":  [
                                                "/node_modules/",
                                                "\u003crootDir\u003e/tests/e2e/"
                                            ],
                 "moduleNameMapper":  {
                                          "^@/(.*)$":  "\u003crootDir\u003e/$1"
                                      }
             },
    "dependencies":  {
                         "@expo/metro-runtime":  "~6.1.2",
                         "@expo/vector-icons":  "^15.0.2",
                         "@react-native-async-storage/async-storage":  "2.2.0",
                         "@react-native-community/datetimepicker":  "8.4.4",
                         "@react-native-picker/picker":  "2.11.1",
                         "@react-navigation/bottom-tabs":  "^7.2.1",
                         "@react-navigation/native":  "^7.0.15",
                         "@react-navigation/stack":  "^7.2.2",
                         "axios":  "^1.8.4",
                         "date-fns":  "^4.1.0",
                         "eventemitter3":  "^5.0.1",
                         "expo":  "~54.0.0",
                         "expo-blur":  "~15.0.7",
                         "expo-constants":  "~18.0.9",
                         "expo-dev-client":  "~6.0.12",
                         "expo-device":  "~8.0.7",
                         "expo-font":  "~14.0.8",
                         "expo-haptics":  "~15.0.7",
                         "expo-image-picker":  "~17.0.8",
                         "expo-linking":  "~8.0.8",
                         "expo-notifications":  "~0.32.11",
                         "expo-router":  "~6.0.6",
                         "expo-secure-store":  "~15.0.7",
                         "expo-splash-screen":  "~31.0.10",
                         "expo-status-bar":  "~3.0.8",
                         "expo-symbols":  "~1.0.7",
                         "expo-system-ui":  "~6.0.7",
                         "expo-updates":  "~29.0.10",
                         "expo-web-browser":  "~15.0.7",
                         "react":  "19.1.0",
                         "react-dom":  "19.1.0",
                         "react-native":  "0.81.4",
                         "react-native-chart-kit":  "^6.12.0",
                         "react-native-feather":  "^1.1.2",
                         "react-native-gesture-handler":  "~2.28.0",
                         "react-native-paper":  "^5.13.1",
                         "react-native-paper-dates":  "^0.22.38",
                         "react-native-reanimated":  "~4.1.0",
                         "react-native-safe-area-context":  "~5.6.0",
                         "react-native-screens":  "~4.16.0",
                         "react-native-svg":  "^15.13.0",
                         "react-native-toast-message":  "^2.2.1",
                         "react-native-vector-icons":  "^10.2.0",
                         "react-native-web":  "^0.21.0",
                         "react-native-webview":  "13.15.0",
                         "react-native-worklets":  "0.5.1",
                         "rxjs":  "^7.8.2"
                     },
    "devDependencies":  {
                            "@babel/core":  "^7.26.3",
                            "@playwright/test":  "^1.55.0",
                            "@types/jest":  "^29.5.12",
                            "@types/node":  "^24.5.1",
                            "@types/react":  "~19.1.10",
                            "@types/react-test-renderer":  "^19.1.0",
                            "babel-plugin-module-resolver":  "^5.0.0",
                            "eslint":  "^9.15.0",
                            "eslint-config-expo":  "~10.0.0",
                            "jest":  "^29.2.1",
                            "jest-expo":  "~54.0.11",
                            "playwright":  "^1.55.0",
                            "react-test-renderer":  "19.1.0",
                            "typescript":  "~5.9.2"
                        },
    "private":  true
}